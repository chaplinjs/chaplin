{
  "name": "chaplin",
  "version": "1.0.0",
  "dependencies": {
    "mocha": "1.13.x",
    "expect": "0.2.x",
    "jquery": "1.9.x",
    "lodash": "2.3.x",
    "backbone": "1.1.x",
    "benchmark": "git://github.com/bestiejs/benchmark.js.git",
    "sinon": "http://sinonjs.org/releases/sinon-1.7.1.js",
    "requirejs": "~2.1.8",
    "davy": "~0.0.4",
    "exoskeleton": "~0.6.0"
  },
  "exportsOverride": {
    "mocha": {
      "scripts/..": "mocha.js",
      "styles/..": "mocha.css"
    },
    "expect": {
      ".": "expect.js"
    },
    "backbone": {
      ".": "backbone.js"
    },
    "requirejs": {
      ".": "require.js"
    },
    "jquery": {
      ".": "jquery.js"
    }
  },
  "main": "build/chaplin.js",
  "ignore": [
    "src",
    "test"
  ]
}
